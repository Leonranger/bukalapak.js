    "babel-core": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "body-parser": "^1.15.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "express": "^4.13.4",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "node-localstorage": "^1.1.2",
